{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**/*.js", "**/*.json"],
        "supportsResponseStreaming": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/socket.io/(.*)",
      "dest": "server.js",
      "headers": {
        "Connection": "Upgrade",
        "Upgrade": "websocket",
        "Access-Control-Allow-Origin": "https://primetime-ruby.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}