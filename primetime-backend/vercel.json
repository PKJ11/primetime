{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "server.js",
      "headers": {
        "Connection": "Upgrade",
        "Upgrade": "websocket"
      }
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "https://primetime_backend_api.vercel.app/socket.io/$1"
    }
  ]
}