{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "https://primetime_backend_apis.vercel.app/socket.io/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "headers": [
    {
      "source": "/socket.io/(.*)",
      "headers": [
        {
          "key": "Connection",
          "value": "Upgrade"
        },
        {
          "key": "Upgrade",
          "value": "websocket"
        }
      ]
    }
  ]
}
